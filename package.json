{
  "name": "aave-governance-ui-helpers",
  "author": "BGD labs",
  "contributors": [
    {
      "name": "Oleksandr Tkach",
      "url": "https://github.com/Argeare5"
    }
  ],
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build:populate-cache": "npx esbuild src/lowdb/populateCache.ts --tsconfig=./tsconfig.json --bundle --outfile=scripts/populate-cache.js --platform=node --target=es6",
    "cache:update": "CACHE_PATH=../src/generated-cache node scripts/populate-cache.js",
    "cache:parse": "node scripts/parseProposalDetails.js"
  },
  "peerDependencies": {
    "@bgd-labs/aave-address-book": ">=2.6.0",
    "ethers": "5.x",
    "@web3-react/core": "~8.2.2",
    "dayjs": "1.x",
    "lodash": "4.x",
    "@types/lodash": "4.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/bgd-aave-governance-ui-helpers.esm.js",
  "size-limit": [
    {
      "path": "dist/bgd-aave-governance-ui-helpers.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/bgd-aave-governance-ui-helpers.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^9.0.0",
    "@types/lodash": "^4.14.198",
    "bignumber.js": "^9.1.2",
    "dotenv": "^16.3.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "ethers": "^5.7.2",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "lowdb": "^6.0.1",
    "prettier": "^3.0.3",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bgd-labs/aave-address-book": "^2.6.0",
    "@web3-react/core": "^8.2.2",
    "dayjs": "^1.11.10",
    "gray-matter": "^4.0.3"
  }
}
